<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="material.pumkinweather.hence.com.pumkinweather">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:name=".main.MainActivity.Main.Activity.MyApplication"
        android:icon="@drawable/pumkin"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".main.MainActivity.Main.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.MainActivity.Main.Activity.DetailsActivity">

        </activity>

    </application>


</manifest>
